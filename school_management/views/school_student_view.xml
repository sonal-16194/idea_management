<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_school2" model="ir.actions.act_window">
        <field name="name">School2</field>
        <field name="res_model">school2.school2</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="view_school2_tree" model="ir.ui.view">
        <field name="name">school2.school2.tree</field>
        <field name="model">school2.school2</field>
        <field name="arch" type="xml">
            <tree string="Schools">
                <field name="name"/>
            </tree>
        </field>
    </record>






    <record id="view_form_school" model="ir.ui.view">
        <field name="name">school2.school2.form</field>
        <field name="model">school2.school2</field>
        <field name="arch" type="xml">
            <form string="School">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                <notebook>
                    <page string="Students">
                        <field name="student_ids">
                            <tree editable="bottom">
                                <field name="s_name"/>
                                <field name="age"/>
                            </tree>
                        </field>
                    </page>
                </notebook>
                </sheet>
            </form>
        </field>
    </record>

    


	<menuitem id="school_management" name="School Management" />
	<menuitem id="menu_realation" name="School Menu" parent="school_management" />
	<menuitem id="sub_menu_school2" name="School Records" parent="menu_realation" action="action_school2" />
</odoo>
